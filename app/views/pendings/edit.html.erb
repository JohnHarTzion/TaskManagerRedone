<p style="font-size: 30px" align="center">Edit Task:</p>

<%= form_for @pendingedit , :html => {:align => "center"} do |f| %>
  <%= f.label :task_name %> <br>
  <%= f.text_field :task_name %><br>

  <%= f.label :task_time_in_minutes %><br>
  <%= f.text_field :task_time_in_minutes %><br>

  <%= f.label :status %><br>
  <%= f.text_field :status %><br>

  <%= f.submit "Edit Task", class:"btn btn-default", type:"button" %>
<% end %>